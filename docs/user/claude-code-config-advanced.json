{
  "mcpServers": {
    "symmetra": {
      "command": "symmetra",
      "args": ["server"],
      "env": {
        "SYMMETRA_LOG_LEVEL": "INFO",
        "OPENAI_API_KEY": "YOUR_OPENAI_API_KEY_HERE",
        "SYMMETRA_SUPABASE_URL": "YOUR_CUSTOM_SUPABASE_URL",
        "SYMMETRA_SUPABASE_KEY": "YOUR_CUSTOM_SUPABASE_ANON_KEY",
        "SYMMETRA_EMBEDDING_BACKEND": "openai"
      }
    }
  }
}